{
  "name": "Fuzium",
  "version": "0.0.8",
  "electronVersion": "1.6.2",
  "license": "MIT",
  "description": "Fusion of Electron & Zeronet",
  "author": "Andrew Kidoo",
  "repository": {
    "type": "git",
    "url": "https://github.com/kustomzone/Fuzium"
  },
  "bugs": {
    "url": "https://github.com/kustomzone/Fuzium/issues"
  },
  "main": "main2.js",
  "scripts": {
    "start": "electron main.js",
    "build":   "electron-packager . Fuzium --platform win32 --arch x64 --electronVersion=1.6.2 && play done.wav",
    "package": "electron-packager . Fuzium --platform win32 --arch=x64 --electronVersion=1.6.2 && play done.wav",
    "setup": "electron-installer-windows --src dist/app-win32-x64/ --dest dist/installers/",
    "test": "node_modules/.bin/standard --verbose js/**/*.js main/*.js"
  },
  "keywords": [
    "fuzium",
    "zeronet",
    "browser",
    "electron",
    "bittorrent"
  ],
  "devDependencies": {
    "asar":                 "^0.12.2",
    "babel-preset-react":   "6.16.0",
    "electron":             "^1.6.2",
    "electron-devtools-installer-offline": "^0.1.4",
    "electron-installer-windows": "^0.2.0",
    "electron-packager":    "^8.5.0",
    "electron-reload":      "^1.1.0",
    "electron-winstaller":  "^2.5.1",
    "grunt":                "^1.0.1",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-uglify": "^0.10.0",
    "grunt-electron":       "^4.0.0",
    "grunt-electron-installer-windows": "^0.2.0",
    "gulp":                 "3.9.1",
    "gulp-babel":           "6.1.2",
    "gulp-compass":         "2.1.0",
    "standard":             "^7.1.0",
    "webpack":              "^1.14.0"
  },
  "dependencies": {
    "asynckit":       "^0.4.0",
    "aws4":           "^1.6.0",
    "commander":      "~2.8.1",
    "configstore":    "~1.2.1",
    "cross-unzip":    "^0.1.0",
    "dexie":          "^1.2.0",
    "dragula":        "^3.6.2",
    "electron-devtools-installer": "^2.0.1",
    "electron-spawn": "^5.0.0",
    "extend":         "^3.0.0",
    "extsprintf":     "^1.3.0",
    "file-loader":    "^0.9.0",
    "file-watcher":   "^0.0.2",
    "is-there":       "^4.3.3",
    "is-typedarray":  "^1.0.0",
    "json-loader":    "^0.5.4",
    "json-schema":    "^0.2.3",
    "jsprim":         "^1.3.1",
    "marked":         "^0.3.6",
    "monogamous":     "~1.0.3",
    "mousetrap":      "^1.5.3",
    "nedb":           "^1.8.0",
    "nlp_compromise": "^1.1.5",
    "node-dir":       "^0.1.16",
    "sshpk":          "^1.11.0",
    "string_score":   "^0.1.22",
    "underscore":     "~1.8.3",
    "verror":         "^1.9.0",
    "winston":        "^2.3.1"
  }
}
